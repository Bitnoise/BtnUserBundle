{% extends "BtnUserBundle::layout.html.twig" %}
{% block body %}

<div class="wrap group">
    <div class="inner-container group">
        <div class="box-hold group">
            <div class="box">
                <div class="box-content">
                    <h1>Login</h1>
                    {% render "BtnUserBundle:Security:loginWithoutTemplate" %}
                </div>
            </div>
        </div>


        <div class="box-hold group">
            <div class="box">
                <div class="box-content">
                    <h1>Register:</h1>
                    {% include "BtnUserBundle:Registration:register.html.twig" %}
                </div>
            </div>
        </div>
    </div>
</div>

{% endblock body %}